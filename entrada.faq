enum Cor { VERMELHO, VERDE, AZUL };

int main {
    Cor cor;
    cor = AZUL;

    switch(cor) {
        case 0: {
            print("Vermelho");
            break;
        }
        case 1: {
            print("Verde");
            break;
        }
        case 2: {
            print("Azul");
            break;
        }
        default: {
            print("Cor desconhecida");
        }
    }

}
























































































































































































































































































































































































































































































































































































































































































